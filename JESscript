    if RealmName == "üèÜ175 Volcano Realm +25 Win" then return("Volcano_Realm") end
    if RealmName == "üèÜ320 Crystal Realm +35 Win" then return("Crystal_Realm") end
    if RealmName == "üèÜ550 Poison Realm +50 Win" then return("Poison_Realm") end
    if RealmName == "üèÜ900 Nuclear Realm +70 Win" then return("Nuclear_Realm") end
    if RealmName == "üèÜ1,450 Lava Realm +100 Win" then return("Lava_Realm") end
